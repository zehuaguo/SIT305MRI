<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/search"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:scrollbarStyle="outsideInset"
    tools:context=".MainActivity">


    <SearchView
        android:id="@+id/searchforrestaurants"
        android:layout_width="392dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="6dp"
        android:layout_marginTop="30dp"
        android:addStatesFromChildren="false"
        android:text="search for restaurants"
        tools:layout_editor_absoluteX="8dp"
        tools:layout_editor_absoluteY="16dp"></SearchView>

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/KoreanBtn"
        android:layout_alignParentStart="true"
        android:layout_marginStart="21dp"
        android:layout_marginTop="15dp"
        android:scrollbarStyle="outsideInset"
        android:text="Korean"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/ChineseBtn"
        android:layout_marginStart="26dp"
        android:layout_marginTop="17dp"
        android:layout_toEndOf="@+id/textView"
        android:scrollbarStyle="outsideInset"
        android:text="Chinese"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="122dp"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/JapaneseBtn"
        android:layout_marginStart="13dp"
        android:layout_marginTop="15dp"
        android:layout_toEndOf="@+id/textView2"
        android:scrollbarStyle="outsideInset"
        android:text="Japanese"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="210dp"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/WesternBtn"
        android:layout_marginStart="14dp"
        android:layout_marginTop="14dp"
        android:layout_toEndOf="@+id/textView3"
        android:scrollbarStyle="outsideInset"
        android:text="Western"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="315dp"
        tools:layout_editor_absoluteY="417dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/AsianBtn"
        android:layout_alignParentStart="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="19dp"
        android:layout_marginBottom="214dp"
        android:scrollbarStyle="outsideInset"
        android:text="Asian"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="32dp"
        tools:layout_editor_absoluteY="577dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/JunkBtn"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="26dp"
        android:layout_marginTop="20dp"
        android:layout_marginBottom="215dp"
        android:layout_toEndOf="@+id/textView5"
        android:scrollbarStyle="outsideInset"
        android:text="Junk food"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="109dp"
        tools:layout_editor_absoluteY="577dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/DessertBtn"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="12dp"
        android:layout_marginTop="21dp"
        android:layout_marginBottom="208dp"
        android:layout_toEndOf="@+id/textView6"
        android:scrollbarStyle="outsideInset"
        android:text="Desserts"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="217dp"
        tools:layout_editor_absoluteY="577dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="72dp"
        android:layout_height="wrap_content"
        android:layout_below="@+id/OthersBtn"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="27dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="207dp"
        android:layout_toEndOf="@+id/textView7"
        android:scrollbarStyle="outsideInset"
        android:text="Others"
        android:textColor="@android:color/background_dark"
        android:textSize="22sp"
        tools:layout_editor_absoluteX="315dp"
        tools:layout_editor_absoluteY="577dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="329dp"
        android:layout_height="119dp"
        android:layout_below="@+id/searchforrestaurants"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="45dp"
        android:visibility="visible"
        tools:layout_editor_absoluteX="27dp"
        tools:layout_editor_absoluteY="87dp"
        tools:srcCompat="@tools:sample/backgrounds/scenic[9]" />

    <ImageButton
        android:id="@+id/KoreanBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/imageView11"
        android:layout_alignParentStart="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="25dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/ChineseBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/imageView11"
        android:layout_marginStart="26dp"
        android:layout_marginTop="26dp"
        android:layout_toEndOf="@+id/KoreanBtn"
        tools:srcCompat="@tools:sample/avatars" />


    <ImageButton
        android:id="@+id/JapaneseBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/imageView11"
        android:layout_marginStart="12dp"
        android:layout_marginTop="31dp"
        android:layout_toEndOf="@+id/ChineseBtn"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/WesternBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/imageView11"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="18dp"
        android:layout_marginTop="35dp"
        android:layout_marginEnd="29dp"
        android:layout_toEndOf="@+id/JapaneseBtn"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/AsianBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/textView"
        android:layout_alignParentStart="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="33dp"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/JunkBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/textView2"
        android:layout_marginStart="21dp"
        android:layout_marginTop="28dp"
        android:layout_toEndOf="@+id/AsianBtn"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/DessertBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/textView3"
        android:layout_marginStart="23dp"
        android:layout_marginTop="31dp"
        android:layout_toEndOf="@+id/JunkBtn"
        tools:srcCompat="@tools:sample/avatars" />

    <ImageButton
        android:id="@+id/OthersBtn"
        android:layout_width="77dp"
        android:layout_height="72dp"
        android:layout_below="@+id/textView4"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="17dp"
        android:layout_marginTop="23dp"
        android:layout_marginEnd="20dp"
        android:layout_toEndOf="@+id/DessertBtn"
        tools:srcCompat="@tools:sample/avatars" />



</RelativeLayout>